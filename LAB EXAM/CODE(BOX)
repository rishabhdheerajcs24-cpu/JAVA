
class Box {
    private double width;
    private double height;
    private double depth;

  
    Box(double len) {
        width = height = depth = len;
    }

   
    Box(double w, double h, double d) {
        width = w;
        height = h;
        depth = d;
    }

    
    Box(Box obj) {
        width = obj.width;
        height = obj.height;
        depth = obj.depth;
    }

 
    double volume() {
        return width * height * depth;
    }
}


class BoxWeight extends Box {
    private double weight;


    BoxWeight(double len, double m) {
        super(len); 
        weight = m;
    }

    BoxWeight(double w, double h, double d, double m) {
        super(w, h, d); 
        weight = m;
    }

    double getWeight() {
        return weight;
    }
}


class BoxShipment extends BoxWeight {
    private double cost;

    
    BoxShipment(double w, double h, double d, double m, double c) {
        super(w, h, d, m); 
        cost = c;
    }

    
    BoxShipment(double len, double m, double c) {
        super(len, m); 
        cost = c;
    }

    double getCost() {
        return cost;
    }
}

public class main {
    public static void main(String[] args) {
        
        BoxShipment shipment = new BoxShipment(10, 20, 15, 34.3, 1200.5);


        System.out.println("Volume  " + shipment.volume());
        System.out.println("Weight  " + shipment.getWeight());
        System.out.println("Cost  " + shipment.getCost());
    }
}
